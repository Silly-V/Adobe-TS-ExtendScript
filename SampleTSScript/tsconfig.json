{
  "compilerOptions": {
    "target" : "ES3",
    "outFile": "build-code/SampleTSScript_CONTENTS.js",
    // "allowJs": true,
    "removeComments": true,
    "skipLibCheck" : true,
    "lib" : ["esnext"],
    "types": [
